<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--持久化对象javabean与数据库表的映射-->
<hibernate-mapping>
	<class name="com.viapro.elec.bean.ElecApplicationTemplate" table="Elec_Application_Template">
		<id name="id" type="long">
			<column name="id"></column>
			<generator class="increment"></generator>
		</id>
		<property name="name" type="string" column="name"></property>
		<property name="processDefinitionKey" type="string" column="processDefinitionKey"></property>
		<property name="path" type="string" column="path"></property>
	</class>
</hibernate-mapping>